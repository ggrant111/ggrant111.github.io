{
  "manifest_version": 3,
  "name": "Lead Generator",
  "version": "1.1",
  "description": "Lead Generator Chrome extension.",
  "manifest_version": 3,
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
      }
  ],
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"]
}